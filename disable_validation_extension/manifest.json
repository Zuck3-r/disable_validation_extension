{
    "manifest_version": 3,
    "name": "Disable Form Validation",
    "version": "1.0",
    "description": "すべてのフォームのクライアントサイドバリデーションを無効にする拡張機能。",
    "permissions": ["activeTab", "scripting"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    }
  }
  